                        <span class="data-list">New</span>

                    </div>
                    <div class="data type">
                        <span class="data-title">Option</span>
                        <div class="flex-btn">
                            <span class="data-list">
                                <button class="delete-btn" id="deletebtn">
                                    <ion-icon name="trash-outline"></ion-icon>
                                </button>
                            </span>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
