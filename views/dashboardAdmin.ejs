<!DOCTYPE html>
<!--=== Coding by CodingLab | www.codinglabweb.com === -->
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!----======== CSS ======== -->
    <link rel="stylesheet" href="/dashboardAdmin.css">

    <!----===== Iconscout CSS ===== -->
    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css">

    <title>Admin Dashboard</title>
</head>

<body>
    <nav class="nav ">
        <div class="logo-name">
            <div class="logo-image">
                <img src="https://vnpayqr.vn/wp-content/uploads/2020/08/30shine-logo.png" alt="">
            </div>

            <span class="logo_name">30ShineAdmin</span>
        </div>

        <div class="menu-items">
            <ul class="nav-links">
                <li><a href="/admin/dashboard">
                        <i class="uil uil-estate"></i>
                        <span class="link-name">Trang chủ</span>
                    </a></li>
                <li><a href="/admin/quan-ly-dat-lich">
                        <i class="uil uil-files-landscapes"></i>
                        <span class="link-name">Quản lý đặt lịch</span>
                    </a></li>
                <li><a href="/admin/san-pham">
                        <i class="uil uil-chart"></i>
                        <span class="link-name">Quản lý sản phẩm</span>
                    </a></li>
                <li><a href="/admin/hanh-trinh-toa-sang">
                        <i class="uil uil-thumbs-up"></i>
                        <span class="link-name">Hành trình toả sáng</span>
                    </a></li>

            </ul>

            <ul class="logout-mode">
                <li><a href="/admin/signin">
                        <i class="uil uil-signout"></i>
                        <span class="link-name">Logout</span>
                    </a></li>

                <li class="mode">
                    <a href="#">
                        <i class="uil uil-moon"></i>
                        <span class="link-name">Dark Mode</span>
                    </a>

                    <div class="mode-toggle">
                        <span class="switch"></span>
                    </div>
                </li>
            </ul>
        </div>
    </nav>

    <section class="dashboard">
        <div class="top">
            <i class="uil uil-bars sidebar-toggle"></i>

            <div class="search-box">
                <i class="uil uil-search"></i>
                <input type="text" placeholder="Search here...">
            </div>

            <img src="images/profile.jpg" alt="">
        </div>

        <div class="dash-content">
            <div class="overview">
                <div class="title">
                    <i class="uil uil-tachometer-fast-alt"></i>
                    <span class="text">Dashboard</span>
                </div>

                <div class="boxes">
                    <div class="box box1">
                        <i class="uil uil-thumbs-up"></i>
                        <span class="text">Lượng khách hàng yêu thích 30shine</span>
                        <span class="number">50,120</span>
                    </div>
                    <div class="box box2">
                        <i class="uil uil-comments"></i>
                        <span class="text">Lượng khách hàng đặt lịch</span>
                        <span class="number">20,120</span>
                    </div>
                    <div class="box box3">
                        <i class="uil uil-share"></i>
                        <span class="text">Chia sẻ từ khách hàng</span>
                        <span class="number">10,120</span>
                    </div>
                </div>
            </div>


            <div class="activity">
                <div class="title">
                    <i class="uil uil-clock-three"></i>
                    <span class="text">Đã tạo gần đây</span>
                </div>

                <div class="activity-data">
                    <!-- <thead>
                        <tr class="info">
                            <th>Id</th>
                            <th>Username</th>
                            <th>Phone</th>
                            <th>Email</th>
                            <th>Password</th>
                            <th>Address</th>
                            <th>Wallet</th>
                            <th>Img</th>
                            <th>Status</th>
                        </tr>
                    </thead> -->
                    <div class="data names">
                        <span class="data-title">Tên Khách Hàng</span>
                        <% for(let i=0;i<userList.length;i++){ %>
                            <span class="data-list">
                                <%=userList[i].name%>
                            </span>

                            <% } %>



                    </div>
                    <div class="data email">
                        <span class="data-title">Số điện thoại</span>
                        <% for(let i=0;i<userList.length;i++){ %>
                            <span class="data-list">
                                <%=userList[i].telephone%>
                            </span>

                            <% } %>
                    </div>
                    <div class="data joined">
                        <span class="data-title">Đã tạo</span>
                        <% for(let i=0;i<userList.length;i++){ %>
                            <span class="data-list">
                                <%= new Date(userList[i].createdAt).toISOString().substring(0, 10) %>
                            </span>

                            <% } %>
                    </div>


                </div>
            </div>
        </div>
    </section>

    <script src="/js/scriptDashAdmin.js"></script>
</body>

</html>